{
    "name": "eoside",
    "displayName": "EOSIDE",
    "version": "1.0.3",
    "publisher": "TOKENIKA",
    "description": "Integrated Development Environment for EOSIO smart contracts",
    "author": {
        "name": "TOKENIKA"
    },
    "categories": [
        "Other"
    ],
    "icon": "media/polygon_gold.png",
    "galleryBanner": {
        "color": "#C80000",
        "theme": "dark"
    },
    "activationEvents": [
        "*",
        "onCommand:eoside.GetStarted"
    ],
    "engines": {
        "vscode": "^1.25.0"
    },
    "main": "./out/src/extension",
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -p ./",
        "watch": "tsc -w -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "devDependencies": {
        "@types/node": "^10.5.2"
    },
    "license": "SEE LICENSE IN LICENSE.txt",
    "bugs": {
        "url": "https://github.com/tokenika/eoside/issues",
        "email": "contact@tokenika.io"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/tokenika/eoside"
    },
    "dependencies": {
        "child_process": "^1.0.2",
        "fs": "^0.0.1-security"
    },
    "extensionDependencies": [
        "ms-vscode.cpptools",
        "ms-python.python"
    ],
    "homepage": "https://github.com/tokenika/eoside",
    "contributes": {
        "configuration": {
            "type": "object",
            "title": "EOSIDE configuration",
            "properties": {
                "eoside.startWithEosIde": {
                    "type": "boolean",
                    "default": true,
                    "description": "If Explorer is empty, start with 'Get Started' view."
                },
                "eoside.menu": {
                    "type": "boolean",
                    "default": true,
                    "description": "Show EOSIDE editor-title menu items."
                },
                "eoside.specialEffects": {
                    "type": "array",
                    "default": [
                        false,
                        false
                    ],
                    "description": "Configure special effects for videos. Do not change it."
                },
                "eoside.testMode": {
                    "type": "boolean",
                    "default": true,
                    "description": "If set, display the native test buttons and test compiler options."
                },
                "eoside.readme": {
                    "type": "string",
                    "default": "https://github.com/tokenika/eosfactory.io/blob/master/eoside/html/_static/1.0.3-native_tests.md",
                    "description": "Set to null to remove the README in the EOSIDE view."
                },
                "eoside.readmeTitle": {
                    "type": "string",
                    "default": "README: native test compilation",
                    "description": "For system use."
                }
            }
        },
        "commands": [
            {
                "command": "eoside.Install",
                "title": "Install",
                "category": "EOSIDE"
            },
            {
                "command": "eoside.GetStarted",
                "title": "| EOSIDE |",
                "category": "EOSIDE"
            },
            {
                "command": "eoside.Setup",
                "title": "| Setup |",
                "category": "EOSIDE"
            },
            {
                "command": "eoside.Compile",
                "title": "compile",
                "category": "EOSIDE"
            },
            {
                "command": "eoside.CompileTest",
                "title": "compile test",
                "category": "EOSIDE"
            },
            {
                "command": "eoside.Build",
                "title": "build",
                "category": "EOSIDE"
            },
            {
                "command": "eoside.BuildTest",
                "title": "build test",
                "category": "EOSIDE"
            },
            {
                "command": "eoside.Deploy",
                "title": "deploy",
                "category": "EOSIDE"
            },
            {
                "command": "eoside.bash",
                "title": "bash terminal",
                "category": "EOSIDE"
            },
            {
                "command": "eoside.Zip",
                "title": "zip",
                "category": "EOSIDE"
            }
        ],
        "keybindings": [
            {
                "command": "eoside.Install",
                "key": "ctrl+alt+i",
                "mac": "cmd+alt+i"
            },
            {
                "command": "eoside.Setup",
                "key": "ctrl+alt+s",
                "mac": "cmd+alt+s"
            },
            {
                "command": "eoside.GetStarted",
                "key": "ctrl+alt+e",
                "mac": "cmd+alt+e"
            },
            {
                "command": "eoside.bash",
                "key": "ctrl+alt+x",
                "mac": "cmd+alt+x"
            },
            {
                "command": "eoside.Compile",
                "key": "ctrl+shift+c",
                "mac": "cmd+shift+c"
            },
            {
                "command": "eoside.CompileTest",
                "key": "ctrl+alt+c",
                "mac": "cmd+alt+c"
            },
            {
                "command": "eoside.Build",
                "key": "ctrl+shift+b",
                "mac": "cmd+shift+b"
            },
            {
                "command": "eoside.BuildTest",
                "key": "ctrl+alt+b",
                "mac": "cmd+alt+b"
            },
            {
                "command": "eoside.Deploy",
                "key": "ctrl+shift+y",
                "mac": "cmd+shift+y"
            },
            {
                "command": "eoside.Zip",
                "key": "ctrl+alt+z",
                "mac": "cmd+alt+z"
            }
        ],
        "menus": {
            "editor/title": [
                {
                    "command": "eoside.GetStarted",
                    "group": "navigation",
                    "when": "config.eoside.menu"
                },
                {
                    "command": "eoside.Setup",
                    "group": "navigation",
                    "when": "config.eoside.menu"
                }
            ]
        }
    }
}
